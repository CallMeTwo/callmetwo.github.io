# Data Analyzer Environment Configuration Example
# Copy this file to .env.local and update values as needed

# Application Settings
VITE_APP_NAME=Data Analyzer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Upload or paste your data to analyze and visualize it

# Environment
NODE_ENV=development
VITE_ENVIRONMENT=development

# Feature Flags
VITE_FEATURE_DATA_VISUALIZATION=false
VITE_FEATURE_STATISTICAL_ANALYSIS=false
VITE_FEATURE_EXPORT_RESULTS=false
VITE_FEATURE_IMPORT_CSV=false
VITE_FEATURE_IMPORT_JSON=false

# Data Configuration
VITE_MAX_DATA_POINTS=10000
VITE_DECIMAL_PLACES=2
VITE_TEXTAREA_HEIGHT=200

# API Configuration (for future backend integration)
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Analytics (optional)
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_ID=

# Chart Configuration (for Recharts)
VITE_CHART_THEME=light
VITE_CHART_RESPONSIVE=true
VITE_CHART_ANIMATION=true

# Data Format Support
VITE_SUPPORTED_FORMATS=csv,tsv,json,plain-text
